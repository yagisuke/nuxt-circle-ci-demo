{
  "name": "nuxt-circle-ci-demo",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "yagisuke <yagisuke.0209@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava",
    "build": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy": "npm run build && gh-pages -d dist -m 'chore: update [skip ci]'"
  },
  "dependencies": {
    "nuxt": "^2.3.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "gh-pages": "^2.0.1",
    "jsdom": "^13.0.0",
    "prettier": "^1.15.2"
  }
}
